<h1>Tickets#new</h1>
<div style="border: 1px solid red;">
  <%= @errors %>
</div>
<%= simple_form_for @ticket do |f|%>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :priority , collection: ['urgent', 'time_sensitive', 'necessary'] %>
  <%= f.input :project_id, collection: @projects%>
  <%= f.input :category %>
  <%= f.input :status %>
  <%= f.button :submit  %>
<% end %>
