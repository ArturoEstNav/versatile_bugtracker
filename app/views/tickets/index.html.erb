<div class="container-layout">
  <div class="navbar-menu">
    <%= render 'shared/navbar_menu' %>
  </div>
  <div class="container-main">
    <h1>All of the tickets
      <span class="button-link button-edit">
        <%= link_to new_ticket_path do %>
          add ticket
        <% end %>
      </span>
    </h1>
    <ul>
      <% @tickets.each do |ticket| %>
        <li class="container-ticket-item">
          <h3><%= ticket.title %></h3>
          <%= link_to "go to ticket", ticket_path(ticket) %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
