<div class="container-layout">
  <div class="navbar-menu">
    <%= render 'shared/navbar_menu' %>
  </div>
  <div class="container-main">
    <h1>My assigned tickets</h1>
    <ul>
      <% @user_tickets.each do |ticket|%>
        <li>
          <h2><%= ticket.title %></h2>
          <%= link_to "go to ticket", ticket_path(ticket) %>
        </li>
      <% end %>
    </ul>
  </div>
</div>






